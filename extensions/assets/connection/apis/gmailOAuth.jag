<%
var googleOAuthUtil = require('/extensions/assets/connection/modules/googleMailOAuthUtil.js').api;
if (request.getMethod() == "POST") {
    var oauth = require("oauth");
    var assetId = request.getParameter("id");

    var oauthService = new oauth.OAuthProvider(googleMailOAuthUtil.getGoogleProviderConfig());

    session.put("oauth_service", oauthService);
    session.put('assetId', assetId);

    var authUrl = oauthService.getAuthorizationUrl();
    response.sendRedirect(authUrl);

} %>